      .onoffswitch-inner:before {
        content: "Yes";
        padding-left: 10px;
        background-color: #f08c33;
        color: #FFFFFF;
        text-align: left;
      }
      .onoffswitch-inner:after {
        content: "No";
        padding-right: 10px;
        background-color: #EEEEEE;
        color: #999999;
        text-align: right;
      }
      input:checked ~ label {
